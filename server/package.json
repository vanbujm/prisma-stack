{
  "name": "atlas",
  "version": "1.0.0",
  "description": "",
  "main": "src/server.js",
  "scripts": {
    "build": "babel src -s -D -d dist --extensions '.js,.jsx,.ts,.tsx'",
    "server": "nodemon --exec yarn run babel-node --require node_modules/dotenv/config --ignore src/worker.ts -- src/server.ts --extensions '.js,.jsx,.ts,.tsx'",
    "worker": "nodemon --exec yarn run babel-node --require node_modules/dotenv/config --watch src/worker.ts -- src/worker.ts --extensions '.js,.jsx,.ts,.tsx'",
    "soap": "nodemon --exec yarn run babel-node -- src/soap.ts --extensions '.js,.jsx,.ts,.tsx'",
    "auspost-checksum": "nodemon --exec yarn run babel-node -- src/auspost-checksum.ts --extensions '.js,.jsx,.ts,.tsx'",
    "start": "node dist/server.js",
    "test": "jest --watch",
    "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn worker\"",
    "cxsd": "cxsd"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ],
    "src/**/*.ts": [
      "tslint --fix",
      "git add"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-link-http": "1.5.9",
    "apollo-server": "2.3.1",
    "apollo-server-express": "2.3.1",
    "bcryptjs": "2.4.3",
    "body-parser": "1.18.3",
    "bull": "3.5.2",
    "bull-arena": "2.5.2",
    "chalk": "git://github.com/chalk/chalk.git",
    "cors": "2.8.5",
    "dotenv": "6.2.0",
    "express": "4.16.4",
    "express-jwt": "5.3.1",
    "graphql": "14.0.2",
    "graphql-middleware": "3.0.0",
    "graphql-shield": "5.1.0",
    "helmet": "3.15.0",
    "javascript-state-machine": "3.1.0",
    "jsonwebtoken": "8.4.0",
    "lodash": "4.17.11",
    "morgan": "1.9.1",
    "pg-boss": "3.1.2",
    "prisma-client-lib": "1.22.2",
    "soap": "^0.25.0"
  },
  "devDependencies": {
    "@babel/cli": "7.2.0",
    "@babel/core": "7.2.0",
    "@babel/node": "7.2.0",
    "@babel/preset-env": "7.2.0",
    "@babel/preset-typescript": "7.1.0",
    "@types/bcryptjs": "2.4.2",
    "@types/bull": "3.5.1",
    "@types/express-jwt": "0.0.40",
    "@types/helmet": "0.0.42",
    "@types/jsonwebtoken": "8.3.0",
    "@types/lodash": "4.14.119",
    "@types/morgan": "1.7.35",
    "@types/node": "10.12.15",
    "babel-core": "6.26.3",
    "babel-jest": "23.6.0",
    "concurrently": "4.1.0",
    "cxsd": "^0.1.1",
    "eslint": "5.10.0",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-prettier": "3.0.0",
    "eslint-plugin-you-dont-need-lodash-underscore": "6.4.0",
    "graphql-import": "0.7.1",
    "husky": "1.2.1",
    "jest": "23.6.0",
    "lint-staged": "8.1.0",
    "nodemon": "1.18.9",
    "prettier": "1.15.3",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.1",
    "tslint-config-prettier": "1.17.0",
    "tslint-eslint-rules": "5.4.0",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "3.6.0",
    "typescript": "3.2.2",
    "xml-formatter": "^1.0.1"
  }
}
