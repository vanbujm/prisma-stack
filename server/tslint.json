{
  "extends": [
    "tslint-config-airbnb",
    "tslint-eslint-rules",
    "tslint-config-prettier",
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": [true, "./.prettierrc"],
    "no-console": false,
    "import-name": false,
    "variable-name": [true, "allow-leading-underscore"],
    "function-name": false
  }
}